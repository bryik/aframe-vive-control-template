{
  "name": "aframe-vive-control-template",
  "version": "0.1.0",
  "description": "Vive Control Template for A-Frame.",
  "main": "index.js",
  "scripts": {
    "build": "browserify examples/main.js -o examples/build.js",
    "dev": "budo examples/main.js:build.js --dir examples --port 8000 --live --open",
    "dist": "webpack index.js dist/aframe-vive-control-template.js && webpack -p index.js dist/aframe-vive-control-template.min.js",
    "lint": "semistandard -v | snazzy",
    "postpublish": "npm run dist",
    "preghpages": "npm run build && shx rm -rf gh-pages && shx mkdir gh-pages && shx cp -r examples/* gh-pages",
    "ghpages": "npm run preghpages && ghpages -p gh-pages"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bryik/aframe-vive-control-template.git"
  },
  "keywords": [
    "aframe",
    "aframe-vr",
    "vr",
    "mozvr",
    "webvr",
    "vive"
  ],
  "author": "bryik <ben@wsundine.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bryik/aframe-vive-control-template/issues"
  },
  "homepage": "https://github.com/bryik/aframe-vive-control-template#readme",
  "devDependencies": {
    "aframe": "aframevr/aframe#master",
    "browserify": "^13.0.0",
    "browserify-css": "^0.9.1",
    "budo": "^8.2.2",
    "ghpages": "^0.0.8",
    "inquirer": "^1.0.2",
    "randomcolor": "^0.4.4",
    "semistandard": "^8.0.0",
    "shelljs": "^0.7.0",
    "shx": "^0.1.1",
    "snazzy": "^4.0.0",
    "webpack": "^1.13.0"
  },
  "semistandard": {
    "ignore": [
      "examples/build.js",
      "dist/**"
    ]
  }
}
